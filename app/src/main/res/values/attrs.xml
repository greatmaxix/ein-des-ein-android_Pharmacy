<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoaderView">
        <attr name="android:tint" />
        <attr name="instantStart" format="boolean" />

        <!-- <attr name="loaderStyle" format="reference"/>-->
        <attr name="progressContainerStyle" format="reference" />
        <attr name="colorBackground" format="reference" />

    </declare-styleable>

    <declare-styleable name="ProfileItemView">
        <attr name="iconProfile" format="reference" />
        <attr name="titleProfile" format="string" />
        <attr name="detailTextProfile" format="string" />
        <attr name="arrowVisibilityProfile" format="boolean" />
        <attr name="mainColorProfile" format="reference" />
    </declare-styleable>

    <declare-styleable name="AppSearchView">
        <attr name="debounce" format="float" />
        <attr name="animationDuration" format="float" />
        <attr name="hintText" format="reference" />
    </declare-styleable>

    <declare-styleable name="EmptyContainerView">
        <attr name="titleEmpty" format="string" />
        <attr name="subtitleEmpty" format="string" />
        <attr name="buttonTextEmpty" format="string" />
        <attr name="isButtonVisible" format="boolean" />
        <attr name="src" format="reference" />
    </declare-styleable>

    <declare-styleable name="AuthNextView">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="AuthFooter">
        <attr name="textDescription" format="string" />
        <attr name="textAction" format="string" />
    </declare-styleable>

    <declare-styleable name="Grayscale">
        <attr name="isDisabled" format="boolean" />
    </declare-styleable>
</resources>